#/bin/bash
EXAMPLE_DIR=exemplos/
EXAMPLE_FILE=maiorDeDoisNumeros.i
RES_FILE=res.msp
make all
cp $EXAMPLE_DIR$EXAMPLE_FILE genI
cd genI
javac gram/Main.java
java gram/Main < $EXAMPLE_FILE > $RES_FILE

cp $RES_FILE ../genMaqV

cd ../
javac energy/EnergyCheckUtils.java energy/Energy.java
mkdir -p genMaqV/energy && cp energy/* genMaqV/energy
cd genMaqV
javac maqv/Main.java
sudo modprobe msr
java maqv/Main $RES_FILE
